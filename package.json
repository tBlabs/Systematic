{
  "name": "systematic",
  "version": "1.0.0",
  "description": "Systematic",
  "main": "index.js",
  "scripts": {
    "serve": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --no-minify --public-url .",
    "test": "jest",
    "test:watch": "jest --watch",
    "zip:win": "del /F dist.zip && @powershell Compress-Archive -Path ./dist/* -DestinationPath dist.zip",
    "push": "curl -X POST -F file=@dist.zip http://deploy.tblabs.pl/systematic",
    "deploy:win": "npm run build && npm run zip:win && npm run push"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/test-shim.js",
      "<rootDir>/config/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/test-preprocessor.js"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "author": "tB",
  "license": "beerware",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "moment": "^2.24.0",
    "parcel-bundler": "^1.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/jasmine": "^3.5.0",
    "@types/jest": "^24.0.24",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/uuid": "^3.4.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "parcel-plugin-typescript": "^0.7.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  }
}
